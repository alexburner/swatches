{
  "name": "swatches",
  "version": "0.1.0",
  "description": "Experimenting with d3 colors",
  "author": "aburner",
  "license": "MIT",
  "main": "src/index.tsx",
  "engine": {
    "node": "9.3.0",
    "npm": "5.6.0"
  },
  "scripts": {
    "watch": "webpack-dev-server",
    "build": "rm -rf docs && webpack -p",
    "clean": "rm -rf .awcache node_modules",
    "prettier": "prettier --write 'src/**/*.+(ts|tsx|js|json)'",
    "tslint": "tslint --fix -p tsconfig.json --exclude 'node_modules/*'",
    "style": "npm run tslint && npm run prettier"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "parser": "typescript"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexburner/swatches.git"
  },
  "dependencies": {
    "@types/d3-interpolate": "1.1.6",
    "@types/d3-scale": "2.0.0",
    "@types/lodash-es": "4.17.0",
    "@types/react-color": "2.13.4",
    "babel-polyfill": "6.26.0",
    "d3-interpolate": "1.1.6",
    "d3-scale": "2.0.0",
    "lodash-es": "4.17.8",
    "qs": "6.5.1",
    "react": "16.2.0",
    "react-color": "2.14.0",
    "react-dom": "16.2.0"
  },
  "devDependencies": {
    "@types/qs": "6.5.1",
    "@types/react": "16.0.36",
    "@types/react-dom": "16.0.3",
    "awesome-typescript-loader": "3.4.1",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "copy-webpack-plugin": "4.4.1",
    "html-webpack-include-assets-plugin": "1.0.2",
    "html-webpack-plugin": "2.30.1",
    "prettier": "1.10.2",
    "source-map-loader": "0.2.3",
    "tslint": "5.9.1",
    "tslint-config-prettier": "1.7.0",
    "tslint-eslint-rules": "4.1.1",
    "tslint-loader": "3.5.3",
    "tslint-react": "3.4.0",
    "typescript": "2.7.1",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1"
  }
}
